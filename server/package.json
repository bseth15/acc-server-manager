{
  "name": "acc-server-manager",
  "version": "0.1.0",
  "description": "Dockerized server management application for Assetto Corsa Competizione built using the MERN stack.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config src/server.js",
    "server": "nodemon -r dotenv/config src/server.js",
    "client": "npm run start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:bseth15/acc-server-manager.git"
  },
  "author": "Seth Brown",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/bseth15/acc-server-manager/issues"
  },
  "homepage": "https://github.com/bseth15/",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
